{
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "groupName": "examples",
      "matchPaths": ["examples/**"],
      "extends": [":semanticCommitTypeAll(chore)"]
    },
    {
      "groupName": "eslint",
      "matchPackagePatterns": ["eslint"]
    },
    {
      "groupName": "jest",
      "matchPackagePatterns": ["jest"]
    }
  ]
}
