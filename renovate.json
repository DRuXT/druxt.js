{
  "extends": [
    "@nuxtjs"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "groupName": "examples",
      "matchPaths": ["examples/**"]
    },
    {
      "groupName": "eslint",
      "matchPackagePatterns": ["eslint"]
    },
    {
      "groupName": "jest",
      "matchPackagePatterns": ["jest"]
    }
  ]
}
