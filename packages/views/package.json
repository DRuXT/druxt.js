{
  "name": "druxt-views",
  "version": "0.13.0",
  "description": "Drupal Views components for Druxt with support for filters, pagination and sorting.",
  "keywords": [
    "cms",
    "decoupled",
    "blocks",
    "drupal",
    "druxt",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "module",
    "router",
    "storybook",
    "vue",
    "views"
  ],
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt-views.ssr.js",
      "import": "./dist/druxt-views.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-views.ssr.js",
  "module": "dist/druxt-views.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^1.1.12",
    "druxt": "^0.10.0",
    "druxt-blocks": "^0.12.0",
    "druxt-entity": "^0.18.0",
    "druxt-router": "^0.20.0",
    "md5": "^2.3.0"
  },
  "optionalDependencies": {
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
