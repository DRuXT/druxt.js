{
  "name": "druxt-schema",
  "version": "0.7.10",
  "description": "Drupal Content Entity schema generator for Druxt with support for View and Form displays.",
  "keywords": [
    "drupal",
    "druxt",
    "schema",
    "entities",
    "field",
    "fields",
    "nuxt"
  ],
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt-schema.ssr.js",
      "import": "./dist/druxt-schema.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-schema.ssr.js",
  "module": "dist/druxt-schema.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "consola": "^2.15.0",
    "drupal-jsonapi-params": "^1.2.2",
    "druxt": "^0.10.0"
  },
  "optionalDependencies": {
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
