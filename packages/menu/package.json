{
  "name": "druxt-menu",
  "version": "0.12.0",
  "description": "Drupal Menu and Menu item Druxt components, with support for the JSON:API Menu Items module.",
  "keywords": [
    "menu",
    "menus",
    "drupal",
    "druxt",
    "nuxt"
  ],
  "license": "MIT",
  "author": "Stuart Clark <stuart@realityloop.com> (realityloop.com)",
  "exports": {
    ".": {
      "require": "./dist/druxt-menu.ssr.js",
      "import": "./dist/druxt-menu.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-menu.ssr.js",
  "module": "dist/druxt-menu.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^1.1.12",
    "druxt": "^0.9.0",
    "druxt-blocks": "^0.11.0"
  },
  "optionalDependencies": {
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
