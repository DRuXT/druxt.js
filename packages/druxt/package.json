{
  "name": "druxt",
  "version": "0.19.0",
  "description": "The Fully Decoupled Drupal Framework for Nuxt.js.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "module",
    "vue"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/druxt.ssr.js",
      "import": "./dist/druxt.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt.ssr.js",
  "module": "dist/druxt.esm.js",
  "files": [
    "dist",
    "stories",
    "templates"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/proxy": "^2.1.0",
    "chalk": "^4.1.2",
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^2.0.0",
    "md5": "^2.3.0",
    "querystring": "^0.2.1",
    "scule": "^0.2.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.4.22"
  },
  "peerDependencies": {
    "axios": "*",
    "consola": "*"
  },
  "optionalDependencies": {
    "core-js": "^3.22.5",
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
