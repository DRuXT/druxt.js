{
  "name": "druxt",
  "version": "0.9.0",
  "description": "The Fully Decoupled Drupal Framework for Nuxt.js.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "module",
    "vue"
  ],
  "homepage": "https://druxtjs.org",
  "bugs": {
    "url": "https://github.com/druxt/druxt.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt.ssr.js",
      "import": "./dist/druxt.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt.ssr.js",
  "module": "dist/druxt.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "deepmerge": "^4.2.2",
    "drupal-jsonapi-params": "^1.2.1",
    "md5": "^2.3.0",
    "querystring": "^0.2.0",
    "scule": "^0.2.0"
  },
  "optionalDependencies": {
    "core-js": "^3.16.1",
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
