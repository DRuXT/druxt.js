{
  "name": "druxt-blocks",
  "version": "0.11.0",
  "description": "Drupal Block and Block Region Druxt components.",
  "keywords": [
    "block",
    "blocks",
    "drupal",
    "druxt",
    "nuxt"
  ],
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt-blocks.ssr.js",
